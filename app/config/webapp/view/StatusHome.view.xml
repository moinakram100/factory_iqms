<mvc:View controllerName="com.ingenx.qms.config.controller.StatusHome"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    xmlns="sap.m">
     <Page id="statusHomePageId" showHeader="false">
      <tnt:ToolHeader
        id="_IDGenToolHeader1"
        class="sapUiTinyMarginTop sapUiTinyMarginEnd sapUiTinyMarginBegin"
    >
        <HBox
            justifyContent="Center"
            width="100%"
        >
            <Title
                id="_IDGenText1"
                text="Parking Statuses"
                level="H3"
                textAlign="Center"
            />
        </HBox>
    </tnt:ToolHeader>
        <content>
            <GenericTile id="statusParkingTile" class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout" header="Update Parking Status" press="onParkingStatus">
                <TileContent id="statusParkingContentId">
                    <ImageContent id="statusParkingIconId" src="sap-icon://BusinessSuiteInAppSymbols/icon-gate" />
                </TileContent>
            </GenericTile>
            <GenericTile id="statusVehicleTile" class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout" header="Update Bay Status" press="onBayStatus">
                <TileContent id="statusVehicleContentId">
                    <ImageContent id="statusVehicleIconId" src="sap-icon://BusinessSuiteInAppSymbols/icon-checklist-item" />
                </TileContent>
            </GenericTile>
</content>
            </Page>
</mvc:View>