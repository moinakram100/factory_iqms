<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        title="Create Vehicle Master"
        id="VehicleMasterDialog"
    >
        <FlexBox class="css1CVM">
            <Table
                id="_IDTable"
                width="28rem"
            >
                <columns>
                    <Column
                        id="_IDColumn1"
                        width="180px"
                        demandPopin="true"
                    >
                        <Label
                            id="_IDLabel1"
                            text="Compartment Details"
                        />
                    </Column>
                    <Column
                        id="_IDColumn2"
                        width="auto"
                        demandPopin="true"
                    >
                        <Label
                            id="_IDLabel2"
                            text="Comp 1"
                        />
                    </Column>
                   
                </columns>
                <items>
                    <ColumnListItem id="_IDColumnListItem1">
                        <cells>
                            <Text
                                id="_IDTex4"
                                text="Max Qty(KL)"
                            />
                            <Input width="100px" id="Maxwt_id" />
                           
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem id="_IDColunListItem1">
                        <cells>
                            <Text
                                id="_IDTx4"
                                text="Min Qty(KL)"
                            />
                            <Input width="100px" />
                           
 
                        </cells>
                    </ColumnListItem>
                   
                </items>
            </Table>
        </FlexBox>
 
        <FlexBox class="css2CVM">
            <Label
                text="Vehicle Details"
                design="Bold"
            />
        </FlexBox>
 
        <FlexBox
            direction="Row"
            class="css3CVM"
        >
            <FlexBox
                direction="Column"
                alignItems="Center"
            >
                 <Label
                    text="Vehicle No :"
                    class="css4CVM"
                />
                <Label
                    text="Type:"
                    class="css5CVM"
                />
                <Label
                    text="TU Number :"
                    class="css6CVM"
                />
                <Label
                    text="Description :"
                    class = "css7CVM"
                   
                />
               
            </FlexBox>
            <FlexBox
                direction="Column"
                alignItems="Center"
                class="cssMaster"
            >
 
                <Input width="150px" id="vehicleNoMaster_id" editable="false"/>
                <Input width="150px" id="Type_id" editable="false"/>
                <Input width="150px" id="TUnumber_id"/>
                <Input width="150px" id="Description_id"/>
               
            </FlexBox>
        </FlexBox>
 
        <Button
            text="Back"
            class="buttonCV"
            press = "onBackVehicleMaster"
        />
        <Button
            text="Submit"
            press="SubmitTuVehicleMaster"
        />
    </Dialog>
</core:FragmentDefinition>
