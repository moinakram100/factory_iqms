{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.ingenx.qms.inventory",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "d8657fc2-2112-4d4a-872f-1d6b690eb8a0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/factory/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ingenx.qms.inventory.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ingenx.qms.inventory.view",
        "async": true,
        "viewPath": "com.ingenx.qms.inventory.view"
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteInventoryDashboard",
          "pattern": "dashboard",
          "target": [
            "TargetInventoryDashboard"
          ]
        },
        {
          "name": "RouteInventoryAnalytics",
          "pattern": "analytics",
          "target": [
            "TargetInventoryAnalytics"
          ]
        }
      ],
      "targets": {
        "TargetHome": {
          "id": "Home",
          "name": "Home"
        },
        "TargetInventoryDashboard": {
          "id": "Dashboard",
          "name": "Dashboard"
        },
        "TargetInventoryAnalytics": {
          "id": "Analytics",
          "name": "Analytics"
        }
      }
    },
    "rootView": {
      "viewName": "com.ingenx.qms.inventory.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
