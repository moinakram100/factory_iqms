<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
 
    <SelectDialog
        id="PlantCodeDialog"
        title="Select Plant"
        liveChange="onPlantSearch"
        search="onPlantSearch"
        confirm="onPlantConfirm"
        items="{/xIQMSxPlants}">
        <StandardListItem id="PlantCodeListItem"
            title="{Plant}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
            description="{Plant_text}"
            wrapping="true"/>
    </SelectDialog>
</core:FragmentDefinition>
