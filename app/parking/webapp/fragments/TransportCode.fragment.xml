<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
 
    <SelectDialog
        id="transporterDialog"
        title="Select Transporter"
        liveChange="onTransportSearch"
        search="onTransportSearch"
        confirm="onTransportConfirm"
        items="{/xIQMSxTransport_n}">
        <StandardListItem id="transportCodeListItem"
            title="{Lifnr}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
            description="{Name1}"
            info="{Ort01}" 
            wrapping="true"/>
    </SelectDialog>
</core:FragmentDefinition>