<mvc:View controllerName="com.ingenx.qms.parking.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
     xmlns:tnt="sap.tnt"
    xmlns="sap.m">
    <tnt:ToolHeader
        id="_IDGenToolHeader2"
        class="sapUiTinyMarginTop sapUiTinyMarginEnd sapUiTinyMarginBegin"
    >
       <HBox
                justifyContent="Center"
                width="100%"
                id="HBoxSCTitle"
            >
                <Title
                    text="Security Clearance"
                    wrapping="true"
                    id="SecurityHome_Header"
                    textAlign="Center"
                    class="headerTitle"
                />
            </HBox>
    </tnt:ToolHeader>

       <Page
        id="parkingPage"
        showHeader="false"
    >
        <content>
            <GenericTile
                id="parkingCreationTile"
                class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout"
                header="Create Parking"
                press="onCreateParkingTile"
            >
                <TileContent id="parkingCreatePATileContent">
                    <ImageContent
                        id="parkingCreatPAImage"
                        src="sap-icon://decision"
                    />
                </TileContent>
            </GenericTile>

            <GenericTile
                id="parkingSecurityTile"
                class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout"
                header="Security Clearance"
                press="onSecurityClearanceTile"
            >
                <TileContent id="parkingTileContent">
                    <ImageContent
                        id="parkingSecurityClearanceImage"
                        src="sap-icon://edit"
                    />
                </TileContent>
            </GenericTile>

            <GenericTile
                id="parkingExitTile"
                class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout"
                header="Exit"
                press="onExitTile"
            >
                <TileContent id="parkingExitTileContent">
                    <ImageContent
                        id="parkingExitImage"
                        src="sap-icon://manager-insight"
                    />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>